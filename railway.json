{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.datadog"
  },
  "deploy": {
    "startCommand": "/otelcol-contrib --config=/etc/otelcol-contrib/otelcol-contrib.yaml",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
